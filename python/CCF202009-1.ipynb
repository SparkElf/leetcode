   "source": [
    "file=open('in.txt','r')\n",
    "input=file.readline\n",
    "n,X,Y=list(map(int,input().split(\" \")))\n",
    "res=[]\n",
    "\n",
    "for i in range(n):\n",
    "    x,y=list(map(int,input().split(\" \")))\n",
    "    d=(X-x)**2+(Y-y)**2\n",
    "    res.append((d,i+1))\n",
    "\n",
    "res.sort()\n",
    "for i in range(3):\n",
    "    print(res[i][1])\n"
   ]
  }
